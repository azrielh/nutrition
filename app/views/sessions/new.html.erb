<div class="container col-xs-6 col-xs-offset-3">
  <h1>Sign in</h1>
  <%= simple_form_for "", url: sessions_path, html: {class: "form-horizontal" }, method: :post do |f| %>
    <%= f.input :email %>
    <%= f.input :password %>
    <%= f.submit "Sign In" , class: "btn btn-primary"%>
  <% end %>
</div>
